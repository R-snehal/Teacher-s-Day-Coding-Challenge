#include <iostream>
#include <string>
#include <vector>
using namespace std;
int main() {
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    string s;
    cin >> s;
    std::vector<bool> seen(26, false);
    int distinct_count = 0;
    for (char c : s) {
        int index = c - 'a';
        if (!seen[index]) {
            seen[index] = true;
            distinct_count++;
        }
    }
    if (distinct_count % 2 != 0) {
        std::cout << "IGNORE HIM!" << std::endl;
    } else {
        std::cout << "CHAT WITH HER!" << std::endl;
    }

    return 0;
}
