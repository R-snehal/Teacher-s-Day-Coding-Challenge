#include <iostream>
#include <string>
using namespace std;
int main() {
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    string s;
    cin >> s;
    int consecutive_count = 1;
    bool dangerous = false;
    if (s.length() < 7) {
        cout << "NO" << endl;
        return 0;
    }
    for (size_t i = 1; i < s.length(); ++i) {
        if (s[i] == s[i-1]) {
            consecutive_count++;
        } else {
            consecutive_count = 1;
        }
        if (consecutive_count >= 7) {
            dangerous = true;
            break;
        }
    }
    if (dangerous) {
        cout << "YES" << endl;
    } else {
        cout << "NO" << endl;
    }
    return 0;
}
