#include <bits/stdc++.h>
using namespace std;
int main() {
    string a;
    cin >> a;
    int n = a.length();
    int zero_index = -1;
    for (int i = 0; i < n; ++i) {
        if (a[i] == '0') {
            zero_index = i;
            break;
        }
    }
    if (zero_index != -1) {
        a.erase(zero_index, 1);
    } else {
        a.pop_back();
    }
    cout << a << endl;
    return 0;
}
