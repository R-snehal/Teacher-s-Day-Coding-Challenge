#include <bits/stdc++.h>
using namespace std;
void solve() {
    int x, y, z;
    cin >> x >> y >> z;
    vector<int> nums = {x, y, z};
    sort(nums.begin(), nums.end());
 
    if (nums[1] == nums[2]) {
        cout << "YES" << endl;
        cout << nums[0] << " " << nums[0] << " " << nums[2] << endl;
    } else {
        cout << "NO" << endl;
    }
}
 
int main() {
    int t;
    cin >> t;
    while (t--) {
        solve();
    }
    return 0;
}
