#include <bits/stdc++.h>
using namespace std;
 
int main() {
    int n;
    cin >> n;
    unordered_map<string, int> db;
 
    while (n--) {
        string name;
        cin >> name;
        if (db.find(name) == db.end()) {
            cout << "OK\n";
            db[name] = 1;
        } else {
            cout << name << db[name] << "\n";
            db[name]++;
        }
    }
 
    return 0;
}
