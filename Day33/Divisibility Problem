#include <iostream>
using namespace std;
void solve() {
    int a, b;
    cin >> a >> b;
    int remainder = a % b;
    if (remainder == 0) {
        cout << 0 << endl;
    } else {
       cout << b - remainder << endl;
    }
}

int main() {
    
    int t;
    cin >> t;
    while (t--) {
        solve();
    }
    
    return 0;
}
