#include <bits/stdc++.h>
using namespace std;
void solve() {
    string s1, s2;
    cin >> s1 >> s2;
    
    string result = "";
    for (int i = 0; i < s1.length(); ++i) {
        if (s1[i] == s2[i]) {
            result += '0';
        } else {
            result += '1';
        }
    }
    cout << result << endl;
}

int main() {
    solve();
    
    return 0;
}
